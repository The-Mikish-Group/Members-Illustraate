<head>

    @* <meta http-equiv="refresh" content="0;url='https://oaksvillageblog.wordpress.com'" /> *@

    @{
        string siteName = Environment.GetEnvironmentVariable("SITE_NAME_ILLUSTRATE")!;
        string siteURL = Environment.GetEnvironmentVariable("SITE_URL_ILLUSTRATE")!;

        ViewData["Title"] = "Notices Blog";
        ViewData["Canonical"] = siteURL + "/Info/Notices";
        ViewData["OGDescription"] = "Explore our community and learn more about us.";

        ViewData["Description"] = ViewData["Title"] + " " + siteName;
        ViewData["OGTitle"] = ViewData["Title"];
        ViewData["OGURL"] = ViewData["Canonical"];
    }

</head>